{
  "name": "zacrac-api",
  "version": "1.0.0",
  "description": "REST api for zacrac",
  "main": "server.ts",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=html mocha src/test/*.js --timeout 10000 -c --require @babel/register --exit",
    "watch-ts": "tsc -w",
    "start": "node dist/server.js",
    "dev": "cross-env nodemon --watch 'src/**/*.ts' --exec ts-node src/server.ts",
    "build": "cross-env rimraf ./dist && tsc && yarn copy-files",
    "copy-files": "cp -r ./src/templates/ ./dist/ && cp -r ./src/assets/ ./dist/",
    "build-ts": "tsc",
    "format": "cross-env prettier --write ./**/*.{js,ts,md,json} --config ./.prettierrc",
    "updateUsers": "node src/updateUser --update"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
